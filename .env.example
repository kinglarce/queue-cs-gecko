# Queue CS Gecko System - Environment Configuration Template
# IMPORTANT: Create a copy of this file named .env and fill in your own secure values

# Supabase Configuration
SUPABASE_URL=http://localhost:8000
SUPABASE_ANON_KEY=your_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here
SUPABASE_PUBLIC_URL=http://localhost:8000

# PostgreSQL Configuration
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_USER=postgres
POSTGRES_DB=postgres
POSTGRES_HOST=postgres
POSTGRES_PORT=5432

# Frontend Configuration
REACT_APP_SUPABASE_URL=http://localhost:8000
REACT_APP_SUPABASE_ANON_KEY=your_anon_key_here
REACT_APP_TITLE=Queue CS Gecko System
REACT_APP_VERSION=1.0.0
REACT_APP_API_TIMEOUT=30000
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_SMS_NOTIFICATIONS=false
REACT_APP_ENABLE_DARK_MODE=true
REACT_APP_DEFAULT_WAIT_TIME_PER_PERSON=3

# JWT Configuration 
JWT_SECRET=your_secure_jwt_secret_here

# API Configuration
API_EXTERNAL_URL=http://localhost:8000
API_RATE_LIMIT_HEADERS=false

# Development Config
REACT_APP_USE_MOCK_API=false

# Studio Configuration
STUDIO_PORT=9000 