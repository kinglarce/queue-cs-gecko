  "private": true,
  "dependencies": {
    "@headlessui/react": "^1.7.15",
    "@heroicons/react": "^2.0.18",
    "@supabase/supabase-js": "^2.39.0",
    "@tanstack/react-query": "^5.8.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "clsx": "^2.0.0",
    "jspdf": "^2.5.1",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-hook-form": "^7.48.2",
    "react-router-dom": "^6.20.0",
    "react-scripts": "5.0.1",
    "react-to-print": "^2.14.15",
    "sonner": "^1.2.0",
    "tailwind-merge": "^2.0.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "uuid": "^9.0.1",
    "web-vitals": "^3.5.0",
    "zustand": "^4.4.6"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:mock": "cross-env REACT_APP_USE_MOCK_API=true react-scripts start",
    "start:supabase": "cross-env REACT_APP_USE_MOCK_API=false react-scripts start",
    "dev": "npm run start:mock",
    "build": "react-scripts build",
    "build:dev": "env-cmd -f .env.development react-scripts build",
    "build:prod": "env-cmd -f .env.production react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css}\"",
    "typecheck": "tsc --noEmit",
    "mock-api": "json-server --watch ../db.json --port 8000"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.10",
    "@types/node": "^20.9.4",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "@types/uuid": "^9.0.7",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "eslint-config-prettier": "^9.0.0",
    "json-server": "^0.17.4",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0"
  }
} 